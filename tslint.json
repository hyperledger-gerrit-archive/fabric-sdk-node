{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs",
			4
		],
		"quotemark": [
			true,
			"single"
		],
		"comment-format": [
			false
		],
		"interface-name": [
			false
		],
		"max-line-length": [
			false
		],
		"max-classes-per-file": false,
		"no-empty-interface": false,
		"variable-name": false
	},
	"rulesDirectory": []
}
